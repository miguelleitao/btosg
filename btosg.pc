prefix=/usr
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib
os=Linux
machtype=x86_64-redhat-linux

Name: btosg
Description: A visual simulation library integrating Bullet and OpenSceneGraph
Version: v1.1.1
URL: https://github.com/miguelleitao/btosg
Requires: bullet openscenegraph-osg openscenegraph-osgViewer openscenegraph-osgSim openscenegraph-osgDB openscenegraph-osgGA openscenegraph-osgShadow
Cflags: -I${includedir}
Libs: -L${libdir} -losg
